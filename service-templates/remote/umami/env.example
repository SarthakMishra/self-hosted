# Umami Analytics Configuration

# === DOMAIN ===
UMAMI_DOMAIN=analytics.example.com

# Redirect URL for root path (optional)
UMAMI_ROOT_REDIRECT_URL=

# === DATABASE ===
POSTGRES_DB=umami
POSTGRES_USER=umami
POSTGRES_PASSWORD=changeme
POSTGRES_INITDB_ARGS=--encoding=UTF-8 --lc-collate=C --lc-ctype=C

# === SECURITY ===
# Generate with: openssl rand -hex 32
UMAMI_APP_SECRET=changeme

# 0 = allow registration, 1 = disable
UMAMI_DISABLE_REGISTRATION=1
UMAMI_FORCE_SSL=1

# === TRACKING ===
UMAMI_TRACKER_SCRIPT_NAME=script.js
UMAMI_BASE_PATH=

# === DEBUGGING ===
UMAMI_DEBUG=0
UMAMI_LOG_QUERY=0
