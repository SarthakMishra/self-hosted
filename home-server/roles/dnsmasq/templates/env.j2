# {{ ansible_managed }}
# DNSmasq Environment Configuration
# Generated by Ansible dnsmasq role

# Domain Configuration
DOMAIN_SUFFIX={{ domain_suffix }}
DNS_PORT={{ dnsmasq_port }}

# Network Configuration
DNSMASQ_NETWORK={{ dnsmasq_network_name }}

# Upstream DNS Servers
UPSTREAM_DNS_1={{ dnsmasq_upstream_dns[0] }}
UPSTREAM_DNS_2={{ dnsmasq_upstream_dns[1] }}

# Cache Configuration
CACHE_SIZE={{ dnsmasq_cache_size }}
TTL={{ dnsmasq_ttl }}

# Resolution Target
RESOLVE_TO={{ dnsmasq_resolve_to }} 