# Vaultwarden Configuration

# === DOMAIN ===
VAULTWARDEN_DOMAIN=vault.example.com

# === SECURITY ===
VAULTWARDEN_SIGNUPS_ALLOWED=false

# Generate with: openssl rand -hex 32
VAULTWARDEN_ADMIN_TOKEN=changeme

# === EMAIL (OPTIONAL) ===
VAULTWARDEN_SMTP_HOST=smtp.example.com
VAULTWARDEN_SMTP_PORT=587
VAULTWARDEN_SMTP_USERNAME=admin@example.com
VAULTWARDEN_SMTP_PASSWORD=changeme
VAULTWARDEN_SMTP_FROM=admin@example.com
