# {{ ansible_managed }}
# Docker Container Log Rotation Configuration
# Generated by Ansible docker_production role

{{ docker_log_rotation.container_logs.path }} {
    rotate {{ docker_log_rotation.container_logs.rotate }}
    daily
    compress
    size={{ docker_log_rotation.container_logs.size }}
    missingok
    delaycompress
    copytruncate
    notifempty
    create 0644 root root
} 